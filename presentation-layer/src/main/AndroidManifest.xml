<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.joshnark.presentation_layer">

    <application>
        <activity
            android:name="com.joshnark.presentation_layer.activities.SplashActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        </activity>

        <activity
            android:name="com.joshnark.presentation_layer.activities.MainActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"
            >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="com.joshnark.presentation_layer.activities.GifDetailActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"
            >
        </activity>
    </application>

</manifest>